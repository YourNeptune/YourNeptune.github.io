<div class="">
  <div class="text-center">
    <h1><%= config.title %></h1>
    <p><%= config.subtitle %></p>
  </div>
  <div class="myPostsContainer">
    <ul>
      <% page.posts.each(function(post){ %>
      <li>
        <h3>
          <a href="<%- url_for(post.path) %>"> <%= post.title %> </a>
        </h3>
        <div class="text-muted"><%= date(post.date, config.date_format) %></div>

        <% if(post.excerpt) { %>
        <p><%- post.excerpt %></p>
        <% } %>
      </li>
      <% }) %>
    </ul>
  </div>
  <!-- Pagination -->
  <% if(page.total > 1) { %>
  <div class="text-center">
    <%- paginator({ prev_text: '<i class="fa fa-angle-left"></i>', next_text:
    '<i class="fa fa-angle-right"></i>', escape: false }) %>
  </div>
  <% } %>
</div>
